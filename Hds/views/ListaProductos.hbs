<h2 style="color: crimson;">Historial</h2>
{{#if productos}}
<div class="table-responsible">
    <table class="table table-dark"> 
        <tr style="color: rgb(4, 0, 255);">
            <th>Nombre</th><th>Precio</th> <th>Foto</th>            
        </tr>
        {{#each productos}}
            <tr>
                <td>eee{{nombre}}</td>
                <td>{{precio}}</td>
                <td>{{Foto}}</td>
            </tr>
        {{/each}}
    </table>
</div>
{{else}}
    <h3 class="alert alert-danger"> No se encontro dato</h3>
{{/if}}
